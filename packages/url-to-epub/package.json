{
  "name": "url-to-epub",
  "version": "1.0.0",
  "description": "Converts URLs to EPUB format using article-fetcher and epub-converter",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "url-to-epub": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint ."
  },
  "dependencies": {
    "article-fetcher": "file:../article-fetcher",
    "epub-converter": "file:../epub-converter"
  }
}
